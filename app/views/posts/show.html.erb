<p id='notice'><%= notice %></p>
<p>
  <strong>Title:</strong>
  <%= @post.title %>
</p>

<p>
  <strong>Body:</strong>
  <%= @post.body %>
</p>

<p>
  <strong>Photo:</strong>
  <%= image_tag @post.image_url %>
</p>
<% if current_user == @user %>
<%= link_to "Edit", edit_user_post_path(@user, @post) %>
<% end %>
<%= link_to "Back", user_posts_path(@user) %>