<h1>User list</h1>
<div class="container my-5">
  <div class="row">
<% @users.each do |user| %>
      <div class="col-md-2 center">
        <div class="avatar">
          <%= image_tag user.image_url(:small) %>
        </div>
        <p> <%= link_to "#{user.name}", user_path(user) %> </p>
      </div>
<% end %>
  </div>
</div>